						</li>
						<li class="submenu">
							<a href="#">
								<i class="fe fe-table"></i> <span> Blog </span> <span class="menu-arrow"></span>
							</a>
							<ul class="submenu_class" style="display: none;">
								<li><a href="/Admin/AllBlog">Blog </a></li>
								<li><a href="/Admin/BlogDetail">Blog View </a></li>
								<li><a href="/Admin/AddBlog">Add Blog </a></li>
								<li><a href="/Admin/EditBlog">Edit Blog </a></li>
							</ul>
						</li>
						<li>
							<a href="/Admin/InvoiceReport"><i class="fas fa-table"></i> <span>Reports</span></a>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<!-- /Sidebar -->
		<!-- Page Wrapper -->
		<div class="page-wrapper">
			<div class="content container-fluid">
				<div class="page-header">
					<div class="row align-items-center">
						<div class="col">
							<h3 class="page-title mt-5">Edit Customer</h3>
						</div>
					</div>
				</div>

				<div class="row">
					<div class="col-lg-12">
						<form asp-action="EditCustomer" asp-controller="Admin" method="post" class="form-horizontal">
							<div class="row formtype">

								<div class="col-md-4">
									<div class="form-group">
										<label>Full Name</label>
										<input type="text" class="form-control" name="FullName" value="@Model.FullName" required />
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Identity Card</label>
										<input type="text" class="form-control" name="IdentityCard" value="@Model.IdentityCard" required />
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Gender</label>
										<select asp-for="Gender" class="form-control" required>
											<option value="">Select</option>
											<option value="Male">Male</option>
											<option value="Female">Female</option>
											<option value="Other">Other</option>
										</select>
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Date of Birth</label>
										<input type="date" class="form-control" name="DOB" value="@Model.DOB.ToString("yyyy-MM-dd")" required />
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Address</label>
										<input type="text" class="form-control" name="Address" value="@Model.Address" required />
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Email</label>
										<input type="email" class="form-control" name="Email" value="@Model.Email" readonly />
									</div>
								</div>

								<div class="col-md-4">
									<div class="form-group">
										<label>Status</label>
										<select asp-for="IsActive" class="form-control" required>
											<option value="true">Active</option>
											<option value="false">Blocked</option>
										</select>
									</div>
								</div>

							</div>

							<div class="text-right mt-3">
								<button type="submit" class="btn btn-primary">Update Customer</button>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
